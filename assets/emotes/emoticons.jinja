/* start destiny.gg */
.chat-emote {
    display: inline-block;
    position: relative;
    z-index: 1;
    top: 10px;
    margin: 0 2px;
    overflow: hidden;
    text-indent: -90000px;
    vertical-align: middle;
}
.chat-emote.chat-emote-OverRustle {
    animation: rustle 100ms 7;
}
.chat-emote.chat-emote-OverRustle:hover {
    animation: rustle-hover 100ms infinite;
}
@keyframes rustle {
    from { left: 0; }
    to { left: 2px; }
}
@keyframes rustle-hover {
    from { left: 0; }
    to { left: 2px; }
}


.chat-emote.chat-emote-MLADY {
    animation: tip 250ms 3;

}
.chat-emote.chat-emote-MLADY:hover {
    animation: tip-hover 250ms infinite;
}
@keyframes tip {
  0% {
    transform: rotate(0deg);
    transform-origin: 4px bottom;
  }
  30% {
    transform: rotate(20deg);
    transform-origin: 4px bottom;
  }
  80% {
    transform: rotate(-10deg);
    transform-origin: 4px bottom;
  }
  100% {
    transform: rotate(0deg);
    transform-origin: 4px bottom;
  }
}
@keyframes tip-hover {
  0% {
    transform: rotate(0deg);
    transform-origin: 4px bottom;
  }
  30% {
    transform: rotate(20deg);
    transform-origin: 4px bottom;
  }
  80% {
    transform: rotate(-10deg);
    transform-origin: 4px bottom;
  }
  100% {
    transform: rotate(0deg);
    transform-origin: 4px bottom;
  }
}


.chat-emote.chat-emote-SpookerZ {
    animation: spook 250ms 3;

}
.chat-emote.chat-emote-SpookerZ:hover {
    animation: spook-hover 250ms infinite;
}
@keyframes spook {
  0% {
    transform: translate(0px 0px);
  }
  25% {
    transform: translate(0px,4px)
  }
  75% {
    transform: rotate(-10deg);
  }
  100% {
    transform: rotate(0deg);
    transform-origin: 4px bottom;
  }
}
@keyframes spook-hover {
  0% {
    transform: translate(0px 0px);
  }
  25% {
    transform: translate(0px,4px)
  }
  75% {
    transform: rotate(-10deg);
  }
  100% {
    transform: rotate(0deg);
    transform-origin: 4px bottom;
  }
}


.chat-emote-DANKMEMES {
  animation: DANKMEMES-anim 1s 2;
}

.chat-emote-DANKMEMES:hover {
  animation: DANKMEMES-anim-hover 1s infinite;
}

@keyframes DANKMEMES-anim {
  0%, 100% {
    -webkit-filter: hue-rotate(0deg);
    filter: hue-rotate(0deg);
  }
  50% {
    -webkit-filter: hue-rotate(360deg);
    filter: hue-rotate(360deg);
  }
}

@keyframes DANKMEMES-anim-hover {
  0%, 100% {
    -webkit-filter: hue-rotate(0deg);
    filter: hue-rotate(0deg);
  }
  50% {
    -webkit-filter: hue-rotate(360deg);
    filter: hue-rotate(360deg);
  }
}


.chat-emote-PICNIC {
  animation: PICNIC-anim 350ms 3;
}

.chat-emote-PICNIC:hover {
  animation: PICNIC-anim-hover 350ms infinite;
}

@keyframes PICNIC-anim {
  0%, 100% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.2);
  }
}

@keyframes PICNIC-anim-hover {
  0%, 100% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.2);
  }
}


.chat-emote-WEEWOO {
  animation: WEEWOO-anim 1.5s 1;
}

.chat-emote-WEEWOO:hover {
  animation: WEEWOO-anim-hover 2s infinite;
}

@keyframes WEEWOO-anim {
  0%, 100% {
    opacity: 1;
  }
  10% {
    opacity: 0.5;
    -webkit-filter: hue-rotate(0deg) ;
    filter: hue-rotate(0deg) ;
  }
  11% {
    -webkit-filter: hue-rotate(170deg) ;
    filter: hue-rotate(170deg) ;
  }
  35% {
    opacity: 1
  }
  50% {
    opacity: 0.2;
    -webkit-filter: hue-rotate(170deg) ;
    filter: hue-rotate(170deg) ;
  }
  51% {
    -webkit-filter: hue-rotate(310deg) ;
    filter: hue-rotate(310deg) ;
  }
  65% {
    opacity: 1;
  }
  90%{
    opacity: 0.2;
    -webkit-filter: hue-rotate(310deg) ;
    filter: hue-rotate(310deg) ;
  }
  91% {
    -webkit-filter: hue-rotate(0deg) ;
    filter: hue-rotate(0deg) ;
  }
}

@keyframes WEEWOO-anim-hover {
  0%, 100% {
    opacity: 1;
  }
  10% {
    opacity: 0.2;
    -webkit-filter: hue-rotate(0deg) saturate(100%);
    filter: hue-rotate(0deg) saturate(100%);
  }
  11% {
    -webkit-filter: hue-rotate(170deg) saturate(200%);
    filter: hue-rotate(170deg) saturate(200%);
  }
  35% {
    opacity: 1
  }
  50% {
    opacity: 0.2;
    -webkit-filter: hue-rotate(170deg) saturate(200%);
    filter: hue-rotate(170deg) saturate(200%);
  }
  51% {
    -webkit-filter: hue-rotate(310deg) saturate(200%);
    filter: hue-rotate(310deg) saturate(200%);
  }
  65% {
    opacity: 1;
  }
  90%{
    opacity: 0.2;
    -webkit-filter: hue-rotate(310deg) saturate(200%);
    filter: hue-rotate(310deg) saturate(200%);
  }
  91% {
    -webkit-filter: hue-rotate(0deg) saturate(100%);
    filter: hue-rotate(0deg) saturate(100%);
  }
}


.chat-emote-Nappa {
  animation: Nappa-anim 750ms 1;
}

.chat-emote-Nappa:hover {
  animation: Nappa-anim-hover 750ms infinite;
}

@keyframes Nappa-anim {
  0% {
    transform: translate(0px 0px) scale(0.5);
  }
  5%,15%,25%,35% {
    transform: translate(0px,3px)
  }
  15%,35%,55% {
    transform: translate(0px,-3px)
  }
  10%,20%,30%,40%,50%{
    transform: translate(3px,0px)
  }
  15.1% {
    transform: scale(0.5);
    -webkit-filter: hue-rotate(-5deg);
    filter: hue-rotate(-5deg);
  }
  30.1% {
    transform: scale(0.75);
    opacity: 0.75;
    -webkit-filter: hue-rotate(-15deg) saturate(125%);
    filter: hue-rotate(-5deg) saturate(125%);;

  }
  60%{
    transform: scale(1.25);
    -webkit-filter: hue-rotate(-25deg) saturate(150%);
    filter: hue-rotate(-25deg) saturate(150%);;

    opacity: 0.75;
  }
  70%{
    opacity: 0;
  }
  90%{
    opacity: 0;
    transform: scale(0.1);
  }
  100%{
    transform: translate(0px,0px) scale(1.0);
    opacity: 1;
  }
}

@keyframes Nappa-anim-hover {
  0% {
    transform: translate(0px 0px) scale(0.5);
  }
  5%,15%,25%,35% {
    transform: translate(0px,3px)
  }
  15%,35%,55% {
    transform: translate(0px,-3px)
  }
  10%,20%,30%,40%,50%{
    transform: translate(3px,0px)
  }
  15.1% {
    transform: scale(0.5);
    -webkit-filter: hue-rotate(-5deg);
    filter: hue-rotate(-5deg);
  }
  30.1% {
    transform: scale(0.75);
    opacity: 0.75;
    -webkit-filter: hue-rotate(-15deg) saturate(125%);
    filter: hue-rotate(-5deg) saturate(125%);;

  }
  60%{
    transform: scale(1.25);
    -webkit-filter: hue-rotate(-25deg) saturate(150%);
    filter: hue-rotate(-25deg) saturate(150%);;

    opacity: 0.75;
  }
  70%{
    opacity: 0;
  }
  90%{
    opacity: 0;
    transform: scale(0.1);
  }
  100%{
    transform: translate(0px,0px) scale(1.0);
    opacity: 1;
  }
}


.chat-emote-Hhhehhehe {
  animation: Hhhehhehe-anim 500ms 1;
  transform-origin: 0 75%;
}

.chat-emote-Hhhehhehe:hover {
  animation: Hhhehhehe-anim-hover 500ms infinite;
}

@keyframes Hhhehhehe-anim {
  0% {
    transform: translate(0px, 0px) rotate(-0deg);
  }
  20% {
    transform: translate(0px, 3px) rotate(-6deg);
  }
  40% {
    transform: translate(0px, 0px) rotate(-0deg);
  }
  60% {
    transform: translate(0px, 3px) rotate(-6deg);
  }
  80% {
    transform: translate(0px, 0px) rotate(-0deg);
  }
  100% {
    transform: translate(0px, 3px) rotate(-6deg);
  }
}

@keyframes Hhhehhehe-anim-hover {
  0% {
    transform: translate(0px, 0px) rotate(-0deg);
  }
  20% {
    transform: translate(0px, 3px) rotate(-6deg);
  }
  40% {
    transform: translate(0px, 0px) rotate(-0deg);
  }
  60% {
    transform: translate(0px, 3px) rotate(-6deg);
  }
  80% {
    transform: translate(0px, 0px) rotate(-0deg);
  }
  100% {
    transform: translate(0px, 3px) rotate(-6deg);
  }
}

